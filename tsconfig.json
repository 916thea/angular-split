{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noEmitHelpers": false,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "noEmitOnError": true,
    "declaration": true,
    "outDir": "dist",
    "rootDir": "src",
    "lib": [
      "es6",
      "es2015",
      "dom"
    ]
  }
}